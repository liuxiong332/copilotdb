{
    "name": "@database-gui/desktop",
    "version": "1.0.0",
    "description": "Database GUI Client - Desktop Application",
    "main": "dist/main/main.js",
    "homepage": "./",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
        "dev:main": "npm run build:main && electron dist/main/main.js --dev",
        "dev:renderer": "vite",
        "build": "npm run build:main && npm run build:renderer",
        "build:main": "tsc -p tsconfig.main.json && tsc -p tsconfig.preload.json",
        "build:renderer": "vite build",
        "dist": "npm run build && electron-builder",
        "dist:win": "npm run build && electron-builder --win",
        "dist:mac": "npm run build && electron-builder --mac",
        "dist:linux": "npm run build && electron-builder --linux",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint . --ext .ts,.tsx",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-slot": "^1.2.3",
        "@supabase/supabase-js": "^2.55.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.12",
        "@tailwindcss/vite": "^4.1.12",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^14.3.1",
        "@types/node": "^24.3.0",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react": "^5.0.2",
        "autoprefixer": "^10.4.21",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "concurrently": "^9.2.1",
        "dom-accessibility-api": "^0.7.0",
        "electron": "^37.4.0",
        "electron-builder": "^26.0.12",
        "eslint": "^9.34.0",
        "jsdom": "^26.1.0",
        "lucide-react": "^0.542.0",
        "postcss": "^8.5.6",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.12",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.9.2",
        "vite": "^7.1.3",
        "vitest": "^3.2.4"
    },
    "build": {
        "appId": "com.database-gui.desktop",
        "productName": "Database GUI Client",
        "directories": {
            "output": "dist-electron"
        },
        "files": [
            "dist/**/*",
            "node_modules/**/*"
        ],
        "mac": {
            "category": "public.app-category.developer-tools",
            "target": "dmg"
        },
        "win": {
            "target": "nsis"
        },
        "linux": {
            "target": "AppImage"
        }
    }
}
